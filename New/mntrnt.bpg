#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = mntrncn mntrackn nodeaddn
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

mntrncn: mntrncn.bpr
  $(MAKE)

mntrackn: mntrackn.bpr
  $(MAKE)

nodeaddn: nodeaddn.bpr
  $(MAKE)


